<h3>Editing Posting: <%= @event.title %></h3>

<% if @flash[:notice] %>
  <div class="notice">
    <%=h @flash[:notice] %>
  </div>
<% end %>

<%= start_form_tag :action => 'update', :id => @event.id %>
  <%= render_partial "form" %>
  <%= submit_tag 'Update' %>
  <%=link_to 'Cancel', @session[:prev_params] %>
<%= end_form_tag %>
